import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc= new Scanner(System.in);
	    int num1=sc.nextInt();
	    int num2=sc.nextInt();
	    int a=num1-num2;
	    if(a%2==0){
	        System.out.println("Even" +  a);
	    }else{
	        System.out.println("odd" +  a);
	    }
	    }
	
	}
